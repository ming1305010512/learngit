<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <!--登陆servlet-->
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.lm.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <!--用户servlet-->
  <servlet>
    <servlet-name>userServlet</servlet-name>
    <servlet-class>com.lm.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userServlet</servlet-name>
    <url-pattern>/userServlet</url-pattern>
  </servlet-mapping>
  <!--用于跳转的servlet-->
  <servlet>
    <servlet-name>transpondServlet</servlet-name>
    <servlet-class>com.lm.servlet.TranspondServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>transpondServlet</servlet-name>
    <url-pattern>/transpondServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>existServlet</servlet-name>
    <servlet-class>com.lm.servlet.ExistServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>existServlet</servlet-name>
    <url-pattern>/existServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>pagingServlet</servlet-name>
    <servlet-class>com.lm.servlet.PagingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pagingServlet</servlet-name>
    <url-pattern>/pagingServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ajaxServlet</servlet-name>
    <servlet-class>com.lm.servlet.AjaxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ajaxServlet</servlet-name>
    <url-pattern>/ajaxServlet</url-pattern>
  </servlet-mapping>
</web-app>
